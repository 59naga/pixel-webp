    "pixel-util": "^0.0.2",
    "prettier": "^1.13.7",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "optionalDependencies": {
    "conventional-github-releaser": "^3.1.2"
  },
