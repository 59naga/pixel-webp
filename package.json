{
  "name": "pixel-webp",
  "description": "Parse Webp to ImageData in cross-platform",
  "files": [
    "index.js",
    "index.mjs",
    "yarn.lock"
  ],
  "scripts": {
    "start": "abby format, test --watch \"*.mjs\"",
